{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "plugins": [
    "lodash",
    "transform-object-rest-spread",
    [
      "angularjs-annotate",
      {
        "explicitOnly": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "__coverage__",
          {
            "ignore": "*.+(test|spec).*"
          }
        ]
      ]
    }
  }
}
